    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun test",
    "test:e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 bun test --test-name-pattern e2e",
